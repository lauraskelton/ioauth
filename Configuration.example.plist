<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>APICredentials</key>
	<dict>
		<key>ClientID</key>
		<string>YOUR_VIMEO_CLIENT_ID_HERE</string>
		<key>ClientSecret</key>
		<string>YOUR_VIMEO_CLIENT_SECRET_HERE</string>
		<key>RedirectURI</key>
		<string>lsioauth://oauth</string>
		<key>AuthURL</key>
		<string>https://api.vimeo.com/oauth/authorize</string>
		<key>TokenURL</key>
		<string>https://api.vimeo.com/oauth/access_token</string>
	</dict>
</dict>
</plist>
